<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.3.2</Version>
    <PackageReleaseNotes>Conversion from SKN/SKL, StaticObject and MapGeometry to glTF</PackageReleaseNotes>
    <PackageIcon>fantome.png</PackageIcon>
    <PackageIconUrl />
    <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/LoL-Fantome/Fantome.Libraries.League</PackageProjectUrl>
    <Authors>Crauzer</Authors>
    <Company>Fantome</Company>
    <Description>Library for Parsing and Editing file formats from League of Legends</Description>
    <AssemblyVersion>1.3.2.0</AssemblyVersion>
    <FileVersion>1.3.2.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="FlatSharp" Version="2.1.0" />
    <PackageReference Include="SharpGLTF.Toolkit" Version="1.0.0-alpha0018" />
    <PackageReference Include="ZstdSharp" Version="0.4.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="fantome.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
